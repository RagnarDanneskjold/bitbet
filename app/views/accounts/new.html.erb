<%= t 'views.accounts.new.title' %> <%= current_user.withdrawal_address || t('views.accounts.new.none') %>
<%= form_for @account do |f| %>
  <%= render 'shared/error_messages', :object => @account %>
  <div class="field">
    <%= f.label :nr, class: "control-label" %>
    <%= f.text_field :nr %>
  </div>
    <div class="actions">
    <%= f.submit I18n.t('views.accounts.new.save_button') %>
  </div>
<% end %>